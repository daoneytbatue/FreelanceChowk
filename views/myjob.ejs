        <!-- Main Content -->
        <div class="main-content">
            <h1>My Jobs</h1>
            <div class="job-tabs">
                <button class="tab-btn active" onclick="showTab('active-jobs')">Active Jobs</button>
                <button class="tab-btn" onclick="showTab('pending-proposals')">Pending Proposals</button>
                <button class="tab-btn" onclick="showTab('completed-jobs')">Completed Jobs</button>
            </div>

            <!-- Active Jobs -->
            <div id="active-jobs" class="job-section">
                <h2>Active Jobs</h2>
                <div class="job-card">
                    <h3>Website Development</h3>
                    <p>Client: John Doe</p>
                    <p>Status: In Progress</p>
                    <p>Payment: $500</p>
                    <button class="btn">Send Message</button>
                    <button class="btn">Mark as Completed</button>
                </div>
                <div class="job-card">
                    <h3>Graphic Design</h3>
                    <p>Client: Jane Smith</p>
                    <p>Status: In Progress</p>
                    <p>Payment: $200</p>
                    <button class="btn">Send Message</button>
                    <button class="btn">Mark as Completed</button>
                </div>
            </div>

            <!-- Pending Proposals -->
            <div id="pending-proposals" class="job-section hidden">
                <h2>Pending Proposals</h2>
                <div class="job-card">
                    <h3>SEO Optimization</h3>
                    <p>Client: Alex Brown</p>
                    <p>Status: Proposal Sent</p>
                    <p>Payment: $300</p>
                    <button class="btn">Send Message</button>
                </div>
            </div>

            <!-- Completed Jobs -->
            <div id="completed-jobs" class="job-section hidden">
                <h2>Completed Jobs</h2>
                <div class="job-card">
                    <h3>Mobile App Development</h3>
                    <p>Client: Alice Green</p>
                    <p>Status: Completed</p>
                    <p>Payment: $1000</p>
                    <button class="btn">Send Message</button>
                </div>
            </div>
        </div>
